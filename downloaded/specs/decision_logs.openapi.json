    "/api/v1/decision_logs/decision_logs": {
      "get": {
        "description": "Returns a collection of decision logs.",
        "operationId": "decision_logs.list_decision_logs",
        "parameters": [
          {
            "in": "query",
            "name": "page.size",
            "schema": {
              "format": "int32",
              "type": "integer"
            }
          },
          {
            "in": "query",
            "name": "page.token",
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "query",
            "name": "policies",
            "schema": {
              "items": {
                "type": "string"
              },
              "type": "array"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/v1ListDecisionLogsResponse"
                }
              }
            },
            "description": "A successful response."
          },
          "default": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/rpcStatus"
                }
              }
            },
            "description": "An unexpected error response."
          }
        },
        "security": [
          {
            "DecisionLogsAPIKey": [],
            "TenantID": []
          }
        ],
        "summary": "List available decision logs",
        "tags": [
          "decision_logs"
        ]
      }
    },
